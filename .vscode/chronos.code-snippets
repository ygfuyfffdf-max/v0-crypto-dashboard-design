  "GSAP ScrollTrigger Timeline": {
    "prefix": "gsapst",
    "body": [
      "import { gsap } from 'gsap'",
      "import { ScrollTrigger } from 'gsap/ScrollTrigger'",
      "",
      "gsap.registerPlugin(ScrollTrigger)",
      "",
      "useLayoutEffect(() => {",
      "  const ctx = gsap.context(() => {",
      "    const tl = gsap.timeline({",
      "      scrollTrigger: {",
      "        trigger: '${1:.section}',",
      "        start: '${2:top top}',",
      "        end: '${3:+=300%}',",
      "        scrub: ${4:1},",
      "        pin: ${5:true},",
      "        snap: {",
      "          snapTo: 'labels',",
      "          duration: 0.3,",
      "          ease: 'power2.inOut'",
      "        }",
      "      }",
      "    })",
      "    ",
      "    tl.addLabel('start')",
      "      .to('${6:.box1}', { x: 500, rotation: 360 })",
      "      .addLabel('middle')",
      "      .to('${7:.box2}', { y: -200, scale: 2 })",
      "      .addLabel('end')",
      "  }, containerRef)",
      "  ",
      "  return () => ctx.revert()",
      "}, [])",
    ],
    "description": "GSAP ScrollTrigger timeline with pin and snap",
  },

