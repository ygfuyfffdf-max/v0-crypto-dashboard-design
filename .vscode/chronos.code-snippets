      "    orientation: '${2:vertical}',",
      "    smoothWheel: true,",
      "    wheelMultiplier: ${3:1},",
      "    touchMultiplier: ${4:2}",
      "  })",
      "  ",
      "  function raf(time: number) {",
      "    lenis.raf(time)",
      "    requestAnimationFrame(raf)",
